<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>TelegramBotFramework</PackageId>
    <Authors>ridicoulous</Authors>
    <Product>TelegramBotFramework</Product>
    <Description>Simple miniframework for easily building Telegram chat bots using modular system.</Description>
    <PackageProjectUrl>https://github.com/ridicoulous/TelegramBotFramework</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ridicoulous/TelegramBotFramework</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>telegram telegram-bot bot-framework</PackageTags>
    <AssemblyVersion>0.0.1.0</AssemblyVersion>
    <FileVersion>0.0.1.0</FileVersion>
    <Version>0.0.1</Version>
    <Copyright>ridicoulous</Copyright>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="2.2.2" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite.Design" Version="1.1.6" />
    <PackageReference Include="Telegram.Bot" Version="14.10.0" />
  </ItemGroup>

</Project>
